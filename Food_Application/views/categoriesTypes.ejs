<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <style>
        @import url("https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css");
    </style>
    <title>Shoping Cart</title>
</head>

<body class="bg-dark">
    <nav class="navbar navbar-expand-lg bg-body-tertiary ">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">Shoping <span class="navbar-toggler-icon"><i class="b bi-cart"></i></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
               
                </span>
            </button>
            <div class="collapse navbar-collapse " id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="/categories">Shoping Cart</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/home">Home</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="/categories">categories</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/addcategories">Add Categories</a>
                    </li>
                  
                    <li class="nav-item">
                        <a class="nav-link" href="/addForms">Add Forms</a>
                    </li>
                </ul>
                <form class="d-flex" role="search">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success" type="submit">Search</button>
                </form>
            </div>
        </div>
    </nav>



    <div class="container">
        <div class="h1 text-center text-primary">Categories of Foods</div>
        <div class="row">

            {{#each categoryName}}
            <div class="col-2 shadow-sm mt-5">
                <div class="card w-100">
                    <img src="/upload/{{this.img}}" style="width:100%;height:200px" class="card-img-top" alt="...">
                    <div class="card-body bg-white text-primary">
                        <h5 class="card-title"><span class="text-danger">Category</span> {{this.name}} </h5>
                        <h5 class="card-title"><span class="text-danger">Type</span> {{this.type}} </h5>
                        {{!-- <h5 class="card-title"><span class="text-danger">Detail</span> {{this.detail}} </h5> --}}
                        <h5 class="card-title"><span class="text-danger">Price</span> {{this.price}} </h5>

                        {{!-- <button href="/shopinglist"  class="btn btn-secondary" data-product="{{this}}">Add</button> --}}
                        {{!-- <a href="/shopinglist/{{this._id}}" class="btn btn-secondary">Add</a> --}}
                    </div>
                </div>
            </div>

            {{/each}}
        </div>

    </div>

    </div>





{{!-- <script src="/static/js/cart.js"></script> --}}


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
